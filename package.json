{
  "name": "@ghaiklor/iterum",
  "version": "0.4.0",
  "description": "Yet another attempt to create VM with its own language and intermediate representation",
  "license": "MIT",
  "homepage": "https://github.com/ghaiklor/iterum#readme",
  "author": {
    "name": "Eugene Obrezkov",
    "email": "ghaiklor@gmail.com",
    "url": "https://ghaiklor.com"
  },
  "bin": {
    "iterum": "./dist/cli/cli.js"
  },
  "bugs": {
    "url": "https://github.com/ghaiklor/iterum/issues",
    "email": "ghaiklor@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghaiklor/iterum.git"
  },
  "directories": {
    "lib": "src",
    "doc": "docs",
    "example": "examples",
    "test": "spec"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "publishConfig": {
    "tag": "latest",
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "build": "npm run clean && npm run lint && npm run test && npm run compile",
    "changelog": "standard-changelog",
    "clean": "rimraf coverage dist",
    "commit": "git-cz",
    "compile": "tsc",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "postversion": "git push && git push --tags",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "preversion": "npm run build",
    "test": "npm run test:unit && npm run test:test262-parser-tests",
    "test:test262-parser-tests": "node tools/test262-parser-tests/test262-parser-tests.js",
    "test:unit": "jest --no-cache --coverage --config jest.config.json",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "commander": "2.20.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.13",
    "@types/node": "12.0.4",
    "chalk": "2.4.2",
    "codecov": "3.5.0",
    "git-cz": "3.0.1",
    "jest": "24.8.0",
    "rimraf": "2.6.3",
    "standard-changelog": "2.0.11",
    "test262-parser-tests": "github:tc39/test262-parser-tests",
    "ts-jest": "24.0.2",
    "tslint": "5.17.0",
    "typescript": "3.5.1"
  }
}
