{
  "name": "iterum",
  "version": "0.0.1",
  "description": "Yet another attempt to create VM with its own language and intermediate representation",
  "license": "MIT",
  "main": "index.js",
  "homepage": "https://github.com/ghaiklor/iterum#readme",
  "author": {
    "name": "Eugene Obrezkov",
    "email": "ghaiklor@gmail.com",
    "url": "https://ghaiklor.com"
  },
  "bugs": {
    "url": "https://github.com/ghaiklor/iterum/issues",
    "email": "ghaiklor@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghaiklor/iterum.git"
  },
  "directories": {
    "lib": "src",
    "doc": "docs",
    "example": "examples",
    "test": "spec"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "build": "npm run clean && npm run lint && npm run test && tsc",
    "clean": "rimraf coverage dist",
    "commit": "npx git-cz",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "test": "npm run test:unit && npm run test:test262",
    "test:test262": "tsc && node tools/test262/test262.js",
    "test:unit": "jest --coverage --config jest.config.json"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "24.0.11",
    "codecov": "3.2.0",
    "cz-conventional-changelog": "2.1.0",
    "jest": "24.3.1",
    "rimraf": "2.6.3",
    "test262-parser-tests": "github:tc39/test262-parser-tests",
    "ts-jest": "24.0.0",
    "tslint": "5.13.1",
    "typescript": "3.3.3333"
  }
}
