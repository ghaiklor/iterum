{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "./src/**/*.ts"
  ],
  "coverageDirectory": "coverage",
  "errorOnDeprecated": true,
  "notify": false,
  "notifyMode": "failure-change",
  "preset": "ts-jest",
  "testEnvironment": "node",
  "testMatch": [
    "**/?(*.)+(spec|test).[jt]s?(x)"
  ],
  "timers": "real",
  "verbose": false
}
